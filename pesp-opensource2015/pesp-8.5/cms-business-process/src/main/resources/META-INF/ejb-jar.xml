<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
  <display-name>Provider Enrollment Services</display-name>
  	<enterprise-beans>
	    <session>
	        <ejb-name>MockMNITSPartnerServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.MockMNITSPartnerServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>RegistrationServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.RegistrationServiceBean</ejb-class>
	        <!--<ejb-local-ref>
	        	<ejb-ref-name>gov.medicaid.services.impl.RegistrationServiceBean/identityProvider</ejb-ref-name>
	        	<ejb-link>DBIdentityProviderDAOBean</ejb-link>
	        </ejb-local-ref>-->
	        <resource-ref>
	        	<res-ref-name>gov.medicaid.services.impl.RegistrationServiceBean/session</res-ref-name>
	        	<res-type>javax.mail.Session</res-type>
	        	<mapped-name>java:/Mail</mapped-name>
	        </resource-ref>
	    </session>
	    <session>
	        <ejb-name>LookupServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.LookupServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>PresentationServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.PresentationServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>ProviderEnrollmentServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.ProviderEnrollmentServiceBean</ejb-class>
            <resource-ref>
                <res-ref-name>gov.medicaid.services.impl.ProviderEnrollmentServiceBean/mqConnectionFactory</res-ref-name>
                <mapped-name>java:/JmsXA</mapped-name>
            </resource-ref>
            <message-destination-ref>
                <message-destination-ref-name>gov.medicaid.services.impl.ProviderEnrollmentServiceBean/dataSyncQueue</message-destination-ref-name>
                <mapped-name>queue/dataSyncQueue</mapped-name>
            </message-destination-ref>
	    </session>
	    <session>
	        <ejb-name>OnboardingServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.OnboardingServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>ExportServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.ExportServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>SequenceGeneratorBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.SequenceGeneratorBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>HibernateAgreementDocumentBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.HibernateAgreementDocumentBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>HelpServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.HelpServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>HibernateEventServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.HibernateEventServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>ScreeningServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.ScreeningServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>ProviderTypeServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.ProviderTypeServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>BusinessProcessServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.BusinessProcessServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>EnrollmentWebServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.EnrollmentWebServiceBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>DBIdentityProviderDAOBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.DBIdentityProviderDAOBean</ejb-class>
	    </session>
	    <session>
	        <ejb-name>FileNetServiceBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.FileNetServiceBean</ejb-class>
	    </session>
    	<message-driven>
	        <ejb-name>DataSynchronizationMessageBean</ejb-name>
	        <ejb-class>gov.medicaid.services.impl.DataSynchronizationMessageBean</ejb-class>
            <activation-config>
                <activation-config-property>
                    <activation-config-property-name>destinationType</activation-config-property-name>
                    <activation-config-property-value>javax.jms.Queue</activation-config-property-value>
                </activation-config-property>
                <activation-config-property>
                    <activation-config-property-name>destination</activation-config-property-name>
                    <activation-config-property-value>queue/dataSyncQueue</activation-config-property-value>
                </activation-config-property>
            </activation-config>
    	</message-driven>
    	    
	</enterprise-beans>
</ejb-jar>